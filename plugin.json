{
  "name": "notification",
  "description": "A plugin for handling notifications",
  "proto": "http",
  "cmd": "go run main.go",
  "endpoint": "localhost:9999",
  "event_key": {
    "include": "^model:",
    "exclude": "artifact"
  },
  "ui_components": [
    {
      "name": "notification",
      "title": "Notification",
      "src": "ui/src/NotificationTab.vue",
      "type": "tab",
      "path": "notification",
      "parent_paths": [
        "/projects/:id",
        "/spiders/:id",
        "/users/:id"
      ]
    },
    {
      "name": "notification-list",
      "title": "Notifications",
      "src": "ui/src/NotificationList.vue",
      "type": "view",
      "path": "notifications"
    },
    {
      "name": "notification-detail",
      "title": "Notifications",
      "src": "ui/src/NotificationDetail.vue",
      "type": "view",
      "path": "notifications/:id"
    }
  ],
  "ui_sidebar_navs": [
    {
      "path": "/notifications",
      "title": "Notifications",
      "icon": [
        "fa",
        "envelope"
      ]
    }
  ],
  "ui_assets": [
    {
      "path": "ui/public/simplemde/simplemde.js",
      "type": "js"
    },
    {
      "path": "ui/public/simplemde/simplemde.css",
      "type": "css"
    },
    {
      "path": "ui/public/css/style.css",
      "type": "css"
    }
  ]
}